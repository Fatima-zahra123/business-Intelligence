                <property
                  dataType="System.String"
                  description="Commande SQL à exécuter."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT     [Entry No_], [Document No_], VE.[Item No_], [Item Ledger Entry No_], ve.[Source No_], 'Consommation' as TypeEcriture, VE.[Location Code], SUM([Cost Amount (Actual)])*-1 AS Cout_Consommation, 
                     0 AS Cout_Production,0 as CoutOperatoire,0 as FraisGenOper, 0 AS Quantité,  SUM([Item Ledger Entry Quantity])*-1 as QuantitéConsommation,
                     0 AS CoutConsmPrevu,0 as QteMatPrevue,0 AS qteRestant,0 AS CoutMatPrevu,max(POC.[Quantity per]) AS [Quantity per],max(POC.[Unit Cost]) AS [Unit Cost],max(POC.[Scrap _]) AS [Scrap %],
                      MAX([Posting Date]) AS posting_Date,0 as poids, '' [Unit of Measure Code], 0 as    [Qty_ per Unit of Measure] 
					 , PO.[Order No_] as 'N° Commande'
FROM         [Staging2020].[dbo].[Staging_Value Entry] VE left join [Staging2020].[dbo].[Staging_Prod_ Order Component] POC on POC.[Prod_ Order No_]=VE.[Document No_] and POC.[Item No_]=VE.[Item No_]
left join [Staging2020].[dbo].[Staging_Production Order] PO ON VE.[Document No_]= PO.No_

WHERE     [Item Ledger Entry Type] in(5)

GROUP BY [Entry No_], [Document No_], VE.[Item No_], [Item Ledger Entry No_], ve.[Source No_],  VE.[Location Code] , PO.[Order No_]

UNION ALL

SELECT     [Entry No_], [Document No_], [Item No_], [Item Ledger Entry No_], VE.[Source No_], 'Production' as TypeEcriture, VE.[Location Code], 0 AS Cout_Consommation, 
                     SUM([Cost Amount (Actual)]) AS Cout_Production,0 as CoutOperatoire,0 as FraisGenOper, SUM([Item Ledger Entry Quantity]) AS Quantité, 0 as QuantitéConsommation,
                      0 AS CoutConsmPrevu,0 as QteMatPrevue,0 AS qteRestant,0 AS CoutMatPrevu, 0 AS [Quantity per],0 AS [Unit Cost],0  AS [Scrap _],
                       MAX([Posting Date]) AS posting_Date,0 as poids, '' [Unit of Measure Code], 0 as    [Qty_ per Unit of Measure] 
					 , PO.[Order No_] as 'N° Commande'

FROM         [Staging2020].[dbo].[Staging_Value Entry] VE left join [Staging2020].[dbo].[Staging_Production Order] PO ON VE.[Document No_]= PO.No_

WHERE     [Item Ledger Entry Type] in(6) 

GROUP BY [Entry No_], [Document No_], [Item No_], [Item Ledger Entry No_], VE.[Source No_],  VE.[Location Code] , PO.[Order No_]

UNION ALL

SELECT     [Entry No_], [Document No_], [Item No_], [Item Ledger Entry No_], VE.[Source No_], 'CoutOper' as TypeEcriture, VE.[Location Code], 0 AS Cout_Consommation, 
                      0 AS Cout_Production,SUM([Cost Amount (Actual)]) as CoutOperatoire,0 as FraisGenOper, SUM([Item Ledger Entry Quantity]) AS Quantité , 0 as QuantitéConsommation,
                      SUM([Expected Operation Cost Amt_]) AS CoutConsmPrevu,0 as QteMatPrevue,0 AS qteRestant,0 AS CoutMatPrevu,0 AS [Quantity per],0 AS [Unit Cost],0 AS [Scrap _],
                       MAX([Posting Date]) AS posting_Date,0 as poids, '' [Unit of Measure Code], 0 as    [Qty_ per Unit of Measure] 

					 , PO.[Order No_] as 'N° Commande'
FROM         [Staging2020].[dbo].[Staging_Value Entry] VE left join [Staging2020].[dbo].[Staging_Prod_ Order Routing Line] PORL on PORL.[Prod_ Order No_]= VE.[Document No_] and PORL.No_=VE.No_
left join [Staging2020].[dbo].[Staging_Production Order] PO ON VE.[Document No_]= PO.No_

WHERE     [Item Ledger Entry Type] in(7)
and [Entry Type]=0

GROUP BY [Entry No_], [Document No_], [Item No_], [Item Ledger Entry No_], VE.[Source No_],  VE.[Location Code] , PO.[Order No_]

UNION ALL

SELECT     [Entry No_], [Document No_], [Item No_], [Item Ledger Entry No_], VE.[Source No_], 'FraisGenOP' as TypeEcriture, VE.[Location Code], 0 AS Cout_Consommation, 
                     0 AS Cout_Production,0 as CoutOperatoire,SUM([Cost Amount (Actual)]) as FraisGenOper, SUM([Item Ledger Entry Quantity]) AS Quantité, 0 as QuantitéConsommation,
                      0 AS CoutConsmPrevu,0 as QteMatPrevue,0 AS qteRestant,0 AS CoutMatPrevu,0 AS [Quantity per], 0 AS [Unit Cost],0 AS [Scrap _],
                       MAX([Posting Date]) AS posting_Date,0 as poids, '' [Unit of Measure Code], 0 as    [Qty_ per Unit of Measure] 

					 , PO.[Order No_] as 'N° Commande'
FROM         [Staging2020].[dbo].[Staging_Value Entry] VE left join [Staging2020].[dbo].[Staging_Production Order] PO ON VE.[Document No_]= PO.No_

WHERE     [Item Ledger Entry Type] in(7)
and [Entry Type]=3

GROUP BY [Entry No_], [Document No_], [Item No_], [Item Ledger Entry No_], VE.[Source No_],  VE.[Location Code] , PO.[Order No_]</property>
